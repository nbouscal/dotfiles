alias ga='git add -A'
alias gap='ga -p'
alias gau='git add -u'
alias gbr='git branch -v'
alias gc='git commit -v'
alias gch='git cherry-pick'
alias gcm='git commit -v --amend'
alias gco='git checkout'
alias gcom='git checkout master'
alias gcop='gco -p'
alias gd='git diff -M'
alias gdc='git diff --cached -M'
alias gds='git diff --stat'
alias gf='git fetch'
alias gfa='git fetch --all'
alias gfm='gf; gco master; gmf origin/master; gco -'
alias gl='. ~/.githelpers && pretty_git_log'
alias gla='gl --all'
alias glr='gl -30'
alias glra='gla -30'
alias gm='git merge --no-ff'
alias gmf='git merge --ff-only'
alias gp='git push'
alias gpo='git push origin'
alias gpom='git push origin master'
alias gr='git reset'
alias grb='git rebase -r'
alias grba='git rebase --abort'
alias grbc='git rebase --continue'
alias grbi='git rebase -i -r --autosquash'
alias grbim='git rebase -i -r --autosquash master'
alias grbm='git rebase -r master'
alias grh='git reset --hard'
alias grp='git remote prune'
alias grpo='git remote prune origin'
alias gs='git status --short'
alias gsh='git show'
alias gst='git stash'
alias gstp='git stash pop'
